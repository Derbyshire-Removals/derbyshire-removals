{
  "name": "derbyshire-removals-11ty",
  "version": "1.0.0",
  "description": "Derbyshire Removals website built with Eleventy",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles/tailwind.css --watch",
    "build": "npm-run-all --sequential build:*",
    "build:css": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles/tailwind.css --minify",
    "build:11ty": "eleventy",
    "clean": "rimraf dist"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "tailwindcss"
  ],
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.16",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1"
  }
}
